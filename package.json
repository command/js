{
  "name": "@oncommandio/js",
  "version": "1.0.0",
  "description": "Command API Wrapper",
  "main": "index.js",
  "scripts": {
    "release": "node ./release.js",
    "build": "babel --presets @babel/preset-env src -d ./ --ignore src/index.test.js && browserify index.js -o index.min.js -t [ babelify --presets [ @babel/preset-env ] --plugins [ @babel/plugin-proposal-object-rest-spread @babel/plugin-proposal-class-properties ] ] && uglifyjs index.min.js -c -m -o index.min.js",
    "dev": "babel --presets @babel/preset-env src -d ./ --watch --ignore src/index.test.js",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oncommandio/js"
  },
  "keywords": [
    "npm",
    "package",
    "template"
  ],
  "author": "oncommandio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cleverbeagle/npm-package-template/issues"
  },
  "homepage": "https://docs.oncommand.io/api/libraries#javascript",
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "aws-sdk": "^2.590.0",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "jest": "^25.0.0",
    "uglify-js": "^3.7.2"
  }
}
